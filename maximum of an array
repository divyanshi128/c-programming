#include <stdio.h>

int main()
{
    int n;
    printf("enter the size of array: ");
    scanf("%d",&n);
    int a[n],max,i;
    printf("enter the elements of array: ");
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    max=a[0];
    for(i=0;i<n;i++){
        if(max<a[i])
        max=a[i];
    }
   printf(“maximum of an array: %d”,max);
   return 0;
}
