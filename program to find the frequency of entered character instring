#include <stdio.h>

int main()
{
    char a[20];
    scanf("%[^\n]%*c",a);
    char ch;
    int count=0;
    printf("enter the character to be searched: ");
    scanf("%c",&ch);
    for(int i=0;a[i];i++){
        if(a[i]==ch){
        count++;}
    }
    printf("the frequency of a searched character is %d",count);
    return 0;
}
