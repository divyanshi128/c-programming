#include <stdio.h>

int main()
{
    int n;
    printf("enter the size of the array:");
    scanf("%d",&n);
    int a[n][n];
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
        printf("enter a[%d][%d]: ",i,j);
        scanf("%d",&a[i][j]);}
    }
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
        printf("%d ",a[j][i]);
        }
        printf("\n");
    }
    return 0;
}

