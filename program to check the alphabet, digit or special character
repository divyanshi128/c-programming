#include <stdio.h>

int main()
{
    char character;
    printf("enter the character: ");
    scanf("%c",&character);
    if(character>='0' && character<='9'){
        printf("entered character is digit");
    }
    else if(character>=65 && character<90 ||character>=97 && character<=122){
        printf("entered character is alphabet");
    }
    else
    printf("entered charachter is special character");
    
    return 0;
}
